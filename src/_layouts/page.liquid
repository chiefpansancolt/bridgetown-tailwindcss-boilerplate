---
layout: default
---

<div class="pb-32">
    {% render "shared/navbar", metadata: site.metadata, navigation: site.data.navigation.main, profile: site.data.navigation.profile, page: page %}

    <header class="bg-gray-50 py-10 text-center">
        <div class="max-w-7xl mx-auto px-4 sm:px-6 lg:px-8">
            <h2 class="text-3xl leading-9 tracking-tight font-extrabold text-gray-900 sm:text-4xl sm:leading-10">
                {{ page.title }}
            </h2>
            <p class="mt-3 max-w-2xl mx-auto text-xl leading-7 text-gray-500 sm:mt-4">
                {{ page.subtitle }}
            </p>
        </div>
    </header>
</div>

<main class="-mt-32">
    {{ content }}
</main>
